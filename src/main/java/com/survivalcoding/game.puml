@startuml
'https://plantuml.com/class-diagram

scale 1.5

skinparam classAttributeIconSize 0
Class Hero{
    + String name
    - int hp

    + void attack(Kinoko enemy)
    + void bye()
    - void die()
}

Class Cleric{
    + String name;
    - int hp
    + int mp

    + void attack(Kinoko enemy)
    + void pray(int sec)
    + void selfAid()
}

@enduml