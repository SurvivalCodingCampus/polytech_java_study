@startuml
'https://plantuml.com/class-diagram

scale 1

class SuperHero extends Hero{
  + boolean isFlying
}

class Character {
   String name
   int hp
   }
class Wizard{
   int hp
   int mp
   }

class Hero extends Character {
      String name
      int hp
      }
class Hero extends Wizard{
      int hp
      int mp
      }


class Horo {
   + String name
   - int hp

   +void attack(Kinoko enemy)
   +void bye()
   -void die()

}
class cleric{
   + String name
   - int hp
   + int mp

   +void attack(Kinoko enemy)
   +void bye()
   -void die()


}

@enduml