@startuml
'https://plantuml.com/class-diagram

class SuperHero extends Hero {
    +boolean isFlying
}

class Hero
{
    +String name
    -int Hp

    +void attack()
    +void bye()
    -void die()
}

class Cleric
{
        +String name
        -int Hp
        +int Mp

        +void attack()
        +void pray(int sec)
        +void selfAid()
}

@enduml